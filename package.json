{
  "name": "asset_assistant",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && node ./bin/as_build.js",
    "preview": "vite preview",
    "deploy": "vite build && node ./bin/as_build.js && cd ./as_build && clasp push",
    "watch": "powershell ./bin/watch.ps1 && vite build --watch"
  },
  "dependencies": {
    "clasp": "^1.0.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0"
  }
}
